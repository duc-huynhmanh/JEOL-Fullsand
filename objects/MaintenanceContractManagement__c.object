    <fields>
        <fullName>LinkBaaNStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF (LastLinkBaaN__c == null, &apos;未連携&apos;, 
  IF (TEXT(LastLinkBaaN__r.clyn__c) == &apos;1&apos;,
      IF (TEXT(LastLinkBaaN__r.ifst__c) == &apos;2&apos;, &apos;取消済&apos;,
      IF (TEXT(LastLinkBaaN__r.ifst__c) == &apos;5&apos;, &apos;取消済（警告あり）&apos;,
      IF (OR (TEXT(LastLinkBaaN__r.ifst__c) == &apos;3&apos;, TEXT(LastLinkBaaN__r.ifst__c) == &apos;4&apos;), &apos;取消失敗&apos;,
      &apos;取消中&apos;
  ))),
      IF (TEXT(LastLinkBaaN__r.ifst__c) == &apos;2&apos;, &apos;連携済&apos;,
      IF (TEXT(LastLinkBaaN__r.ifst__c) == &apos;5&apos;, &apos;連携済（警告あり）&apos;,
      IF (OR (TEXT(LastLinkBaaN__r.ifst__c) == &apos;3&apos;, TEXT(LastLinkBaaN__r.ifst__c) == &apos;4&apos;), &apos;連携失敗&apos;,
      &apos;連携中&apos;
  )))
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
