                <table id="viewTable" class="record-list" cellspacing="0" cellpadding="0" style="border-collapse: collapse; margin-top:0px; margin-bottom:0px;">
                    <col class="clm00" />
                    <col class="clm01" />
                    <col class="clm02" />
                    <col class="clm03" />
                    <col class="clm04" />
                    <col class="clm05" />
                    <col class="clm06" />
                    <col class="clm07" />
                    <col class="clm08" />
                    <apex:repeat value="{!MaintenanceContractManagements}" var="MaintenanceContractManagement" >
                        <tr class="contents">

                            <!-- COLONNE 1 -->
                            <td>
                              <apex:outputText value="{!baseRowNumber+MaintenanceContractManagement.rowNumber}" />
                            </td>

                            <!-- COLONNE 2 -->
                            <td>
                              <apex:outputLink styleClass="field-body" value="{!$Page.JEOLAsia_MaintenanceDetail}" >
                                <apex:param name="id" value="{!MaintenanceContractManagement.rec.id}" />
                                <apex:outputText value="{!$Label.JEOLAsia_MaintList_Label_Edit}" />
                              </apex:outputLink>
                            </td>

                            <!-- COLONNE 3 -->
                            <td >
                                <apex:outputLink styleClass="field-body" value="{!$Page.JEOLAsia_MaintenanceDetailView + '?id=' + MaintenanceContractManagement.rec.id}" >
                                    <apex:outputText value="{!MaintenanceContractManagement.rec.Name}" />
                                </apex:outputLink>
                                <br/>
                                <apex:outputField value="{!MaintenanceContractManagement.rec.NewOdrSalesPipeline__c}" />
                                <br/>
                                <apex:outputLink styleClass="field-body" value="{!$Page.JEOLAsia_ProjectMaster}?id={!MaintenanceContractManagement.rec.ProjectMaster__c}" >
                                    <apex:outputField value="{!MaintenanceContractManagement.rec.ProjectMaster__r.Name}" />
                                </apex:outputLink>
                            </td>

                            <!-- COLONNE 4 -->
                            <td>
                                <apex:outputField value="{!MaintenanceContractManagement.rec.NewOdrFlagshipProduct__c}" />
                            </td>

                            <!-- COLONNE 5 -->
                            <td >
                                <apex:outputField value="{!MaintenanceContractManagement.rec.NewOdrSalesAccount__c}" />
                                <br/>
                                <apex:outputField value="{!MaintenanceContractManagement.rec.NewOdrShipmentAccount__c}" />
                            </td>

                            <!-- COLONNE 6 -->
                            <td >
                              <apex:outputText value="{0, date, yyyy/MM/dd}">
                                  <apex:param value="{!MaintenanceContractManagement.rec.LastActivityDate}"/>
                              </apex:outputText>
                            </td>

                            <!-- COLONNE 7 -->
                            <td >
                              <apex:outputText rendered="{!MaintenanceContractManagement.rec.ContractPrice__c <> NULL}" value="{!MaintenanceContractManagement.rec.CurrencyContractPrice__c}" />&nbsp;
                              <apex:outputField value="{!MaintenanceContractManagement.rec.ContractPrice__c}" />
                              <br/>
                              <apex:outputField value="{!MaintenanceContractManagement.rec.JuchuKakudoValue__c}" />
                            </td>

                            <!-- COLONNE 8 -->
                            <td style="text-align: center;">
                              <apex:outputField value="{!MaintenanceContractManagement.rec.QuotationCreated__c}" />
                              <br/>
                              <apex:outputText value="{0, date, yyyy/MM/dd}">
                                  <apex:param value="{!MaintenanceContractManagement.rec.QuotationSentDate__c}"/>
                              </apex:outputText>
                            </td>

                            <!-- COLONNE 9 -->
                            <td style="text-align: center;">
                              <apex:outputField value="{!MaintenanceContractManagement.rec.SACreated__c}" />
                              <br/>
                              <apex:outputText value="{0, date, yyyy/MM/dd}">
                                  <apex:param value="{!MaintenanceContractManagement.rec.ContractSentDate__c}"/>
                              </apex:outputText>
                            </td>
                        </tr>
                    </apex:repeat>
                </table>
            </div>
            <!-- ページ遷移欄-->
            <table class="record-list" border="1" cellspacing="0" cellpadding="0" style="border-collapse: collapse; margin-top:0px; margin-bottom:0px;">
              <col class="clm00" />
              <col class="clm01" />
              <col class="clm02" />
              <col class="clm03" />
              <col class="clm04" />
              <col class="clm05" />
              <col class="clm06" />
              <col class="clm07" />
              <col class="clm08" />
              <apex:variable value="{!MaintenanceContractManagements.size}" var="size" />
              <tr class="menu">
                <td colspan="9" style="padding: 0;">
                  <table id="each_menu">
                    <tr>
                      <td align="left" width="33%" style="border: 0px; vertical-align: middle;" >
                        <apex:outputPanel rendered="{!ssc.PageSize != null}">
                          <apex:outputText value="{!(ssc.PageNumber - 1) * ssc.PageSize + 1} - {!(ssc.PageNumber - 1) * ssc.PageSize + size}/{!ssc.ResultSize}" />
                        </apex:outputPanel>
                        <apex:outputText value="     {!$Label.JEOLAsia_MaintList_Label_ShowBefore}" />
                        <apex:selectList id="pageSize" value="{!pageSize}" size="1" disabled="{!ssc.ResultSize < 9}">
                          <apex:actionSupport event="onchange" action="{!setSizeSaveCurrentPageSize}" rerender="searchResult" oncomplete="mode();"/>
                          <apex:selectOption itemValue="10" itemLabel="10"/>
                          <apex:selectOption itemValue="20" itemLabel="20"/>
                          <apex:selectOption itemValue="50" itemLabel="50"/>
                        </apex:selectList>
                        <apex:outputLink value="javascript:void(0);" onclick="allViewMode();">{!$Label.JEOLAsia_MaintList_Label_ShowAfter}</apex:outputLink>
                      </td>
                      <td align="center" width="33%" style="border: 0px; vertical-align: middle ;">
                        <apex:outputLink value="javascript:void(0)" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + 1 != 1}">
                          <apex:actionSupport event="onclick" action="{!firstAndSaveCurrentPageNumber}" rerender="searchResult" onsubmit="showWaitPanel();" oncomplete="mode();hideWaitPanel();" />
                          <apex:outputText value="｜◀" />
                        </apex:outputLink>
                        <apex:outputText value="｜◀" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + 1 == 1}"/>
                        <apex:outputText value=" " />
                        <apex:outputLink value="javascript:void(0)" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + 1 != 1}" >
                          <apex:actionSupport event="onclick" action="{!previousAndSaveCurrentPageNumber}" rerender="searchResult" onsubmit="showWaitPanel();" oncomplete="mode();hideWaitPanel();" />
                          <apex:outputText value="◀ {!$Label.JEOLAsia_MaintList_Label_Previous}" />
                        </apex:outputLink>
                        <apex:outputText value="◀ {!$Label.JEOLAsia_MaintList_Label_Previous}" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + 1 == 1}"/>
                        <apex:outputText value=" " />
                        <apex:outputLink value="javascript:void(0)" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + size != ssc.ResultSize}">
                          <apex:actionSupport event="onclick" action="{!nextAndSaveCurrentPageNumber}" rerender="searchResult" onsubmit="showWaitPanel();" oncomplete="mode();hideWaitPanel();" />
                          <apex:outputText value="{!$Label.JEOLAsia_MaintList_Label_Next} ▶" />
                        </apex:outputLink>
                        <apex:outputText value="{!$Label.JEOLAsia_MaintList_Label_Next} ▶" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + size == ssc.ResultSize}"/>
                        <apex:outputText value=" " />
                        <apex:outputLink value="javascript:void(0)" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + size != ssc.ResultSize}">
                          <apex:actionSupport event="onclick" action="{!lastAndSaveCurrentPageNumber}" rerender="searchResult" onsubmit="showWaitPanel();" oncomplete="mode();hideWaitPanel();" />
                          <apex:outputText value="▶｜" />
                        </apex:outputLink>
                        <apex:outputText value="▶｜" rendered="{!(ssc.PageNumber - 1) * ssc.PageSize + size == ssc.ResultSize}"/>
                      </td>
                      <td align="right" width="33%" style="border: 0px; vertical-align: middle ;">
                        <apex:outputText value="{!$Label.JEOLAsia_MaintList_Label_Page}" />
                        <apex:input type="number" value="{!ssc.PageNumber}" size="2" style="width: 40px;" disabled="{!ssc.PageSize == 0}">
                          <apex:actionSupport event="onchange" action="{!setJumpSaveCurrentPageSize}" rerender="searchResult" oncomplete="mode();"/>
                        </apex:input>
                      </td>
                    </tr>
                  </table>
                </td>
              </tr>
            </table>
        </apex:outputPanel>
        <apex:outputpanel rendered="{!MaintenanceContractManagements.size == null || MaintenanceContractManagements.size <= 0}" >
            <table class="record-list" border="1" cellspacing="0" cellpadding="0" style="border-collapse: collapse; margin-top: 0; margin-bottom: 0;">
              <tr class="contents_title">
                <td >
                  <div class="in_contents_title">{!$Label.JEOLAsia_MaintList_Message_NoData}</div>
                </td>
              </tr>
            </table>
        </apex:outputpanel>
    </apex:outputPanel>
  </apex:form>
</apex:page>