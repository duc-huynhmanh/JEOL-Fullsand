        </apex:outputLink>
        <apex:outputLink value="{!$Page.JEOLAsia_MaintenanceListToRenewAdd}" styleClass="customButton">
            ＋&nbsp;{!$Label.JEOLAsia_MaintList_Label_AddManually}
        </apex:outputLink>
      </div>
    </div>
