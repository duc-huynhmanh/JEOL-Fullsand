                      <!-- 予定開始日 -->
                      <td class="label labelLarge">
                            <apex:outputText value="Schedule Start Date"/>
                      </td>
                      <td colspan="3">
                        <!-- <apex:input type="date" value="{!periodFrom}" style="width: 120px;"/> -->
                        <apex:inputText value="{!periodFrom}" id="periodFromId" style="width: 120px;" onfocus="DatePicker.pickDate(false, '{!$Component.periodFromId}', false);"/>
                        <apex:outputText value="〜" />
