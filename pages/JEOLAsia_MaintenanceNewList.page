                        <apex:outputText value="SA created"/>
                      </td>
                      <td>
                        <apex:selectList styleClass="input" value="{!saCreated}" style="width: 132px;" size="1">
                          <apex:selectOptions value="{!ListSACreated}" />
                        </apex:selectList>
                      </td>

                      <!-- 住所 -->
                      <td class="label labelSmall">
                            <apex:outputText value="Address"/>
                      </td>
                      <td>
                        <apex:inputText value="{!shipmentAddress}" id="shipmentAddress" styleClass="input" maxlength="255" />
                      </td>

                    </tr>

                    <!-- FILTERS - ROW 3 -->
                    <tr>

                      <!-- 対応日付 -->
                      <td class="label labelLarge">
                            <apex:outputText value="Last Activity Date"/>
                      </td>
                      <td colspan="3">
