                      </td>
                      <td>
                        <apex:inputText value="{!orderProbabilityFrom}" id="orderProbabilityFrom" styleClass="input" maxlength="3" style="width: 48px;"/>
                        <apex:outputText value="〜" />
                        <apex:inputText value="{!orderProbabilityTo}" id="orderProbabilityFTo" styleClass="input" maxlength="3" style="width: 48px;"/>
                      </td>

                    </tr>

                    <!-- FILTERS - ROW 6 -->
                    <tr>

                      <!--  系列 -->
                      <td class="label labelLarge">
                        <apex:outputText value="{!$Label.JEOLAsia_MaintList_Label_Series}" />
                      </td>
                      <td>
                        <apex:actionFunction name="seriesSelect" oncomplete="showSeriesSelect();" rerender="seriesSelectPanel" />
