                            <apex:selectOptions value="{!ListAccountTypeName}"/>
                        </apex:selectRadio>
                      </td>
                      <td>
                        <apex:inputText value="{!customerName}" id="customrName" styleClass="input" maxlength="255" />
                      </td>

