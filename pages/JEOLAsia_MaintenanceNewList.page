                      </td>

                      <td class="label">
                        <apex:outputText value="Selected Series" />
                      </td>
                      <td colspan="3" style="max-width: 300px;">
                        <div style="width: 100%; overflow: hidden; vertical-align: middle; text-overflow: ellipsis; white-space: nowrap; line-height: 29px;">
                          <apex:outputText value="{!SelectedSeries}" id="SelectedSeries"/>
                        </div>
                      </td>
                    </tr>

                </table>

                <div id="buttons" >
                    <apex:commandLink value="{!$Label.JEOLAsia_MaintList_Label_Clear}" styleClass="button_white" action="{!doFormClear}" rerender="search_criteria_form, searchResult" style="text-decoration: none;" />
                    <apex:commandLink value="{!$Label.JEOLAsia_MaintList_Label_Search}" styleClass="button_green" action="{!doSearch}" rerender="searchResult" style="text-decoration: none;" onclick="showWaitPanel();" oncomplete="hideWaitPanel();" />
