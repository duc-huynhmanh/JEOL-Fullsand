                        <apex:outputText value="〜" />
                        <!-- <apex:input type="date" value="{!quotationSentDateTo}" style="width: 120px;"/> -->
                        <apex:inputText value="{!quotationSentDateTo}" id="quotationSentDateToId" style="width: 120px;" onfocus="DatePicker.pickDate(false, '{!$Component.quotationSentDateToId}', false);"/>
                      </td>

                    </tr>

                    <!-- FILTERS - ROW 5 -->
