                    </td>
                  </tr>
                  <tr>
                    <td>
                    </td>
                    <td colspan="4">
                      <table>
                        <tr>
                          <td>
                            <table class="radio-table">
                              <tr>
                                <td style="padding-left: 10px;">
                                  <img src="{!IF(CapitalOrExecSomeCountryItems[0] == rec.CapitalOrExecSomeCountry__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                                  <apex:outputText value="{!CapitalOrExecSomeCountryItems[0]}" styleClass="field-label" />
<!-- CapitalOrExecSomeCountryLabel -->
                                  (
                                  <apex:outputField id="CapitalOrExecSomeCountryLabel" styleClass="field-value CapitalOrExecSomeCountryLabel other" value="{!rec.CapitalOrExecSomeCountryLabel__c}" />
                                  )
                                </td>
                                <td style="padding-left: 10px;">
                                  <img src="{!IF(CapitalOrExecSomeCountryItems[1] == rec.CapitalOrExecSomeCountry__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                                  <apex:outputText value="{!CapitalOrExecSomeCountryItems[1]}" styleClass="field-label" />
                                </td>
                                <td style="padding-left: 10px;">
                                  <img src="{!IF(CapitalOrExecSomeCountryItems[2] == rec.CapitalOrExecSomeCountry__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                                  <apex:outputText value="{!CapitalOrExecSomeCountryItems[2]}" styleClass="field-label" />
                                </td>
                              </tr>
                            </table>
                          </td>
                        </tr>
                      </table>
                    </td>
                  </tr>
<!-- 12.CapitalOrExecSomeCountry -->
                  <tr>
                    <td>
                      <label for="No" class="field-label No" >12.</label>
                    </td>
