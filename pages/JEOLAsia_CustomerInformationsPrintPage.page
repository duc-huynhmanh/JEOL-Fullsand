                        <tr>
                          <td colspan="2">
                            <img src="{!IF(CustomerFieldItems[7] == rec.CustomerField__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerFieldItems[7]}" styleClass="field-label" />
