<apex:page renderAs="pdf" showHeader="false" standardStylesheets="false" applyHtmlTag="false" applyBodyTag="false" doctype="html-5.0" controller="JEOLAsia_CustomerInfoPrintPageCtrl">

  <apex:outputPanel layout="none" id="contentPage">
    <html>
      <head>
        <meta charset="utf-8" />
        <title>Salesforce - JEOLAsia_CustomerInformations</title>

        <style>
            @page {
                size: A4;
                margin-top: 30pt;
                margin-bottom: 38pt;
                @bottom-right {
                    content: "9606211B(04)";
                    padding-bottom: 10pt;
                    font-size: 9pt;
                    font-family: 'Arial Unicode MS';
                }
            }
          body {
            margin: 0pt;
            padding: 0pt;
            font-size: 9pt;
            font-family: 'Arial Unicode MS';
            line-height: 9px;
          }

          .contents {
            width: 100%;
            margin: 0 auto;
          }
          .field-label {
            margin: 1px;
          }
          .field-src {
            margin: -5px;
          }

          .contents .field .field-value {
            margin: 1px;
          }
          .radio-table-title {
            vertical-align: top;
          }

          .field-label.checkLabel {
            border-style: solid;
            border-width: 1px;
            border-color: black;
          }
          .field-label.noCheckLabel {
            border-style: solid;
            border-width: 0px;
            border-color: black;
          }
          hr {
            margin-top: 0px; 
            margin-bottom: -5px;
          }

        </style>
      </head>

      <body>
        <div class="contents">

            <apex:form id="form" style="margin-bottom: 0;padding-bottom: 0;">

              <div class="screenname" style="text-decoration: underline; text-align: center; font-weight: bold;">
                <apex:outputText value="CUSTOMER INFORMATION" />
              </div>
              <br/>
              <div class="field" style="padding: 0 0 0 10px;" >
<!-- 1.Order No. -->
                <table>
                  <col span="1" style="width:10px;"/>
                  <col span="1" style="width:180px;"/>
                  <col span="1" style="width:100px;"/>
                  <col span="1" style="width:110px;"/>
                  <col span="1" style="width:290px;"/>

                  <tr>
                    <td>
                      <label for="No" class="field-label No" >1.</label>
                    </td>
                    <td>
                      <label for="OrderNo" class="field-label OrderNo" >Order No.  :</label>
                    </td>
                    <td>
                      <apex:outputField id="OrderNo" styleClass="field-value OrderNo" value="{!rec.OrderNo__c}" />
                    </td>
<!-- Date -->
                    <td>
                      <label for="OrderDate" class="field-label OrderDate" >Date : </label>
                      <apex:outputField id="OrderDate" styleClass="field-value OrderDate" value="{!rec.OrderDate__c}" />
                    </td>
                    <td style="line-height: 10px;" >
                      <apex:outputField value="{!rec.Revision__c}"/>
                    </td>
                  </tr>
<!-- 2.Instrument -->
                  <tr>
                    <td>
                      <label for="No" class="field-label No" >2.</label>
                    </td>
                    <td>
                      <label for="Instrument" class="field-label Instrument" >Instrument  :</label>
                    </td>
                    <td colspan="3">
                      <apex:outputField id="Instrument" styleClass="field-value Instrument" value="{!rec.Instrument__c}" />
                    </td>
                  </tr>
<!-- 3.Configuration -->
                  <tr>
                    <td class="radio-table-title">
                      <label for="No" class="field-label No" >3.</label>
                    </td>
                    <td class="radio-table-title">
                      <label for="Configuration" class="field-label Configuration" >Configuration  :</label>
                    </td>
                    <td colspan="3" style="line-height: 10px;">
                      <apex:outputField id="Configuration" style="width: 750px; margin-bottom: 4px;" styleClass="field-value Configuration" value="{!rec.Configuration__c}"/>
                    </td>
                  </tr>
<!-- 4.Customer's Name -->
                  <tr>
                    <td>
                      <label for="No" class="field-label No" >4.</label>
                    </td>
                    <td>
                      <label for="CustomerName" class="field-label CustomerName" >Customer's Name  :</label>
                    </td>
                    <td colspan="3">
                      <apex:outputField id="CustomerName" styleClass="field-value CustomerName" value="{!rec.CustomerName__c}"/>
                    </td>
                  </tr>
<!-- 5.Address -->
                  <tr>
                    <td class="radio-table-title">
                      <label for="No" class="field-label No" >5.</label>
                    </td>
                    <td class="radio-table-title">
                      <label for="Address" class="field-label Address" >Address  :</label>
                    </td>
                    <td colspan="3" style="line-height: 1.1em;">
                      <apex:outputField id="Address" style="width: 750px; margin-bottom: 4px;" styleClass="field-value Address" value="{!rec.Address__c}"/>
                    </td>
                  </tr>
<!-- Nation -->
                  <tr>
                    <td colspan="3">
                    </td>
                    <td style="text-align: right;">
                      <label for="Nation" class="field-label Nation" >Nation  :</label>
                    </td>
                    <td>
                      <apex:outputField id="Nation" styleClass="field-value Nation" value="{!rec.Nation__c}"/>
                    </td>
                  </tr>
                  <tr>
                    <td colspan="4">
                    </td>
                    <td colspan="1">
                      <hr/>
                    </td>
                  </tr>
<!-- URL -->
                  <tr>
                    <td>
                    </td>
                    <td style="text-align: right;">
                      <label for="URL" class="field-label URL" >(URL)  :</label>
                    </td>
                    <td colspan="3" style="line-height:1.2em;">
                        <apex:outputText value="{!dispUrl}" escape="false" />
                    </td>
                  </tr>
                  <tr>
                    <td colspan="2">
                    </td>
                    <td colspan="3">
                      <hr/>
                    </td>
                  </tr>
                </table>
<!-- 6.Customer's Status -->
                <table>

                  <col span="1" style="width:10px;"/>
                  <col span="1" style="width:180px;"/>
                  <col span="1" style="width:600px;"/>

                  <tr>
                    <td class="radio-table-title">
                      <label for="No" class="field-label No" >6.</label>
                    </td>
                    <td class="radio-table-title">
                      <label for="CustomerStatus" class="field-label CustomerStatus" >Customer's Status  :</label>
                    </td>
                    <td>
                      <table class="radio-table">
                        <tr>
                          <td>
                            <img src="{!IF(CustomerStatusItems[0] == rec.CustomerStatus__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerStatusItems[0]}" styleClass="field-label" style="position: relative;"/>
                          </td>
                          <td>
                            <img src="{!IF(CustomerStatusItems[1] == rec.CustomerStatus__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerStatusItems[1]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(CustomerStatusItems[2] == rec.CustomerStatus__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerStatusItems[2]}" styleClass="field-label" />
                          </td>
                        </tr>
                      </table>
                      <table class="radio-table">
                        <tr>
                          <td>
                            <img src="{!IF(CustomerStatusItems[3] == rec.CustomerStatus__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerStatusItems[3]}" styleClass="field-label" />
                          </td>
                          <td style="width:250px;">
                            <img src="{!IF(CustomerStatusItems[4] == rec.CustomerStatus__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerStatusItems[4]}" styleClass="field-label" />
                          </td>
                        </tr>

                      </table>
                    </td>
                  </tr>
<!-- 7.Type of Facility -->
                  <tr>
                    <td class="radio-table-title" >
                      <label for="No" class="field-label No" >7.</label>
                    </td>
                    <td class="radio-table-title" >
                      <label for="FacilityType" class="field-label FacilityType" >Type of Facility  :</label>
                    </td>
                    <td>
                      <table class="radio-table">
                        <tr>
                          <td>
                            <img src="{!IF(FacilityTypeItems[0] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[0]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(FacilityTypeItems[1] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[1]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(FacilityTypeItems[2] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[2]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(FacilityTypeItems[3] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[3]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(FacilityTypeItems[4] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[4]}" styleClass="field-label" />
                          </td>
                        </tr>
                        <tr>
                          <td>
                            <img src="{!IF(FacilityTypeItems[5] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[5]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(FacilityTypeItems[6] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[6]}" styleClass="field-label" />
                          </td>
                          <td colspan="3">
                            <img src="{!IF(FacilityTypeItems[7] == rec.FacilityType__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!FacilityTypeItems[7]}" styleClass="field-label" />
<!-- Type of Facility Label -->
                            (
                            <apex:outputField id="FacilityTypeOtherLabel" styleClass="field-value FacilityTypeOtherLabel other" value="{!rec.FacilityTypeOtherLabel__c}"/>
                            )
                          </td>
                        </tr>
                      </table>

                    </td>
                  </tr>
<!-- 8.Field of Customer -->
                  <tr>
                    <td class="radio-table-title" >
                      <label for="No" class="field-label No" >8.</label>
                    </td>
                    <td class="radio-table-title" >
                      <label for="CustomerField" class="field-label CustomerField" >Field of Customer  :</label>
                    </td>
                    <td>
                      <table class="radio-table" >
                        <tr>
                          <td>
                            <img src="{!IF(CustomerFieldItems[0] == rec.CustomerField__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
                            <apex:outputText value="{!CustomerFieldItems[0]}" styleClass="field-label" />
                          </td>
                          <td>
                            <img src="{!IF(CustomerFieldItems[1] == rec.CustomerField__c, '/img/checkbox_checked.gif', '/img/checkbox_unchecked.gif')}" class="field-src"/>
